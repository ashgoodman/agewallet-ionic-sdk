{
  "name": "agewallet-ionic-demo",
  "version": "1.0.0",
  "description": "AgeWallet Ionic SDK Demo App",
  "private": true,
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=dist/main.js --format=esm --platform=browser && cp src/index.html dist/",
    "start": "npx serve dist",
    "cap:init": "npx cap init 'AgeWallet Demo' io.agewallet.sdk.demo.ionic --web-dir dist",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "agewallet-ionic-sdk": "file:.."
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "esbuild": "^0.24.0",
    "serve": "^14.0.0",
    "typescript": "^5.0.0"
  }
}
